{
    "block": {
        "enabled": true,
        "verticalPadding": "1em",
        "horizontalPadding": "1em",
        "border": "1px solid luiTheme(element, divider, color)",
        "invertedBorder": "rgba(255, 255, 255, 0.2)",
        "borderRadius": "3px"
    },
    "breadcrumbs": {
        "enabled": true,
        "itemPadding": "0.5em 0",
        "dividers": {
            "fontFamily": "''",
            "character": "003e",
            "margin": "0 0.5em",
            "color": "luiTheme(element, divider, color)"
        }
    },
    "button": {
        "enabled": true,
        "minHeight": "1em",
        "minWidth": "3em",
        "verticalPadding": "0.66em",
        "horizontalPadding": "1em",
        "verticalMargin": "0.25em",
        "horizontalMargin": "0.25em",
        "textTransform": "normal",
        "textShadow": "none",
        "borderRadius": "3px",
        "hoverShadow": "lui_theme_shadow()",
        "defaultColoring": {
            "color": "#FFFFFF",
            "text": "luiTheme(element, typography, body, color)"
        },
        "defaultBorderWidth": "1px",
        "defaultBorderColor": "luiTheme(element, divider, color)",
        "inverted": {
            "color": "rgba(200, 200, 200, 0.75)"
        },
        "flat": {
            "background": "transparent",
            "textDecoration": "underline",
            "hoverBackground": "rgba(0, 0, 0, 0.05)",
            "activeBackground": "rgba(0, 0, 0, 0.1)",
            "activeShadow": "0 0 0 1px rgba(50, 50, 50, .1)",
            "invertedHoverBackground": "rgba(200, 200, 200, 0.1)",
            "invertedActiveBackground": "rgba(255, 255, 255, .25)"
        },
        "wired": {
            "borderWidth": "2px"
        },
        "disabledOpacity": 0.5,
        "disabledGrayscale": "50%",
        "addon": {
            "width": "2.5em",
            "background": "rgba(0, 0, 0, 0.1)"
        }
    },
    "callout": {
        "enabled": true,
        "background": "#FFF",
        "borderColor": "luiPalette(light, color)",
        "borderWidth": "5px",
        "verticalPadding": "1em",
        "horizontalPadding": "2em",
        "verticalMargin": "1em",
        "horizontalMargin": 0,
        "defaultColoring": {
            "color": "luiPalette(light, color)",
            "text": "#222"
        }
    },
    "day-block": {
        "enabled": true,
        "width": "5em",
        "verticalMargin": "1em",
        "horizontalMargin": 0,
        "textTransform": "uppercase",
        "textAlign": "center",
        "textColor": "luiTheme(element, typography, body, color)",
        "borderRadius": "luiTheme(adjective, rounded, radius)",
        "borderWidth": "1px",
        "borderColor": "luiTheme(element, divider, color)",
        "day": {
            "verticalPadding": "0.25em",
            "fontSize": "3em",
            "fontWeight": 600
        },
        "month": {
            "verticalPadding": 0,
            "fontSize": "1.5em",
            "fontWeight": 600
        },
        "year": {
            "verticalPadding": "0.1em",
            "fontSize": "1em",
            "fontWeight": 400
        }
    },
    "divider": {
        "enabled": true,
        "width": "1px",
        "fatWidth": "3px",
        "color": "rgba(50, 50, 50, 0.2)",
        "invertedColor": "rgba(255, 255, 255, 0.2)"
    },
    "grid": {
        "enabled": true,
        "gutter": "1em",
        "columns": 12,
        "containerWidth": "auto"
    },
    "icon": {
        "enabled": true,
        "createClasses": true,
        "iconBaseSelector": "'#{$prefix}.icon'"
    },
    "input": {
        "enabled": true,
        "background": "#FFF",
        "borderRadius": "3px",
        "color": "#666",
        "padding": {
            "top": "0.66em",
            "right": "1em",
            "bottom": "0.66em",
            "left": "1em"
        },
        "borderWidth": "1px",
        "borderColor": "luiPalette(light, color)",
        "focusBorderColor": "luiPalette(light, color, dark)",
        "invalidColor": "luiPalette(red, color)",
        "shadow": "0 2px 2px 0 luiTheme(element, divider, color)",
        "placeholder": {
            "color": "rgba(0, 0, 0, 0.5)",
            "style": "italic"
        },
        "field": {
            "verticalMargin": "0.5em",
            "labelFontWeight": 600
        },
        "lengths": {
            "x-short": "5em",
            "short": "8em",
            "medium": "15em",
            "long": "20em",
            "x-long": "25em"
        },
        "checkbox": {
            "enabled": true,
            "color": "luiPalette(primary, color)",
            "size": "1.5em"
        },
        "radio": {
            "enabled": true,
            "color": "luiPalette(primary, color)",
            "size": "1.5em",
            "lineHeight": 1.5
        },
        "switch": {
            "enabled": true,
            "width": "4.5em",
            "height": "2.25em",
            "background": "#666",
            "color": "darken(#666, 15)",
            "activeBackground": "luiPalette(primary, color)",
            "activeColor": "luiPalette(primary, color, dark)",
            "grabLinesColor": "luiPalette(light, color)"
        },
        "search": {
            "enabled": true,
            "paddingLeft": "2.5em",
            "tag": {
                "padding": "0.25em 0.5em",
                "margin": "0.1em",
                "background": "luiPalette(light, color, light)",
                "borderColor": "luiPalette(light, color)",
                "borderRadius": "luiTheme(adjective, rounded, radius)",
                "selectedColor": "luiPalette(primary, color)"
            }
        },
        "timespanpicker": {
            "enabled": true,
            "width": "5em"
        },
        "momentpicker": {
            "enabled": true,
            "width": "2.2em",
            "maxWidth": "5.25em"
        },
        "translations": {
            "background": "#FFF",
            "dropdownPadding": "0.5em",
            "zIndex": 3
        }
    },
    "label": {
        "enabled": true,
        "fontSize": "0.85em",
        "lineHeight": 1,
        "textAlign": "center",
        "verticalPadding": "0.25em",
        "horizontalPadding": "0.5em",
        "verticalMargin": 0,
        "horizontalMargin": "0.5em",
        "borderRadius": "3px",
        "defaultColoring": {
            "color": "luiPalette(light, color)",
            "text": "#666"
        },
        "addonBackground": "rgba(0, 0, 0, 0.2)",
        "iconMargin": "0.25em"
    },
    "menu": {
        "enabled": true,
        "defaultColoring": {
            "color": "#666",
            "text": "#FFF"
        },
        "verticalMargin": 0,
        "horizontalMargin": 0,
        "verticalPadding": 0,
        "horizontalPadding": 0,
        "borderColor": "luiTheme(element, divider, color)",
        "invertedBorderColor": "rgba(255, 255, 255, 0.2)",
        "borderStyle": "solid",
        "borderWidth": "1px",
        "item": {
            "vertical": {
                "verticalPadding": "lui_rem(1)",
                "horizontalPadding": "lui_rem(1.5)",
                "verticalMargin": 0,
                "horizontalMargin": 0
            },
            "horizontal": {
                "verticalPadding": "lui_rem(1)",
                "horizontalPadding": "lui_rem(1.5)",
                "verticalMargin": 0,
                "horizontalMargin": 0
            },
            "header": {
                "fontSize": "''"
            }
        },
        "dividing": {
            "itemColor": "luiTheme(element, typography, body, color)",
            "dividerColor": "luiTheme(element, divider, color)",
            "dividerWidth": "1px",
            "activeDividerWidth": "luiTheme(element, divider, fatWidth)",
            "activeDividerColor": "luiPalette(primary, color)"
        },
        "tabbed": {
            "padding": "0 lui_rem(1)",
            "background": "#FAFAFA",
            "activeBackground": "#FFF",
            "borderColor": "luiTheme(element, divider, color)",
            "activeBorderWidth": "3px",
            "bottomSpacing": "5px"
        },
        "pills": {
            "borderRadius": "3px"
        }
    },
    "notify": {
        "enabled": true,
        "maxWidth": "90%",
        "zIndex": 9999
    },
    "overlay": {
        "enabled": true,
        "zIndex": 100,
        "background": "rgba(0, 0, 0, 0.5)"
    },
    "progress": {
        "enabled": true,
        "height": "1.5em",
        "slimHeight": "5px",
        "verticalMargin": "1em",
        "borderRadius": "luiTheme(adjective, rounded, radius)",
        "background": "#EFEFEF",
        "defaultColoring": {
            "color": "#CCC",
            "text": "#666"
        },
        "status": {
            "fontWeight": 600
        },
        "progressBar": {
            "zIndex": 1000,
            "height": "2px",
            "fadeInDuration": "0.25s"
        }
    },
    "table": {
        "enabled": true,
        "background": "#fff",
        "border": "1px solid '#d6d6d6'",
        "padding": "0.5em 1em",
        "zebraBackground": "rgba(200, 200, 200,0.25)",
        "verticalMargin": "1em",
        "horizontalMargin": 0,
        "textAlignment": "left",
        "coloredBorderWidth": "5px"
    },
    "tablegrid": {
        "background": "#FFF",
        "textAlignment": "left",
        "toolbar": {
            "background": "luiPalette(grey, color)",
            "text": "luiPalette(grey, text)"
        },
        "header": {
            "background": "luiPalette(light, color, light)",
            "borderColor": "luiPalette(light, color)",
            "text": "luiPalette(light, text, light)",
            "cellPaddingTop": "2em",
            "fontWeight": "normal",
            "textAlignment": "left"
        },
        "rows": {
            "zebraColor": "luiPalette(light, color, light)",
            "hoverColor": "luiPalette(primary, color, x-light)"
        },
        "cell": {
            "minWidth": "5em",
            "padding": "0.5em 1em"
        }
    },
    "typography": {
        "enabled": true,
        "body": {
            "fontFamily": "'Source Sans Pro', 'Tahoma', Verdana, Geneva, Helvetica, Arial, sans-serif",
            "fontSize": "13px",
            "color": "#666",
            "lineHeight": "1.4em",
            "boldWeight": 600
        },
        "smallSize": "luiTheme(reference, sizes, small)",
        "headings": {
            "fontFamily": "'Source Sans Pro', 'Tahoma', Verdana, Geneva, Helvetica, Arial, sans-serif",
            "lineHeight": "1.25em",
            "verticalMargin": 0,
            "fontWeight": "normal",
            "h1": {
                "fontSize": "2.75em",
                "lineHeight": "1.25em",
                "fontWeight": 300
            },
            "h2": {
                "fontSize": "2.25em",
                "lineHeight": "1.25em",
                "fontWeight": 300
            },
            "h3": {
                "fontSize": "1.8em",
                "lineHeight": "1.3em",
                "fontWeight": 400
            },
            "h4": {
                "fontSize": "1.5em",
                "lineHeight": "1.3em",
                "fontWeight": 400
            },
            "h5": {
                "fontSize": "1.25em",
                "lineHeight": "1em",
                "fontWeight": 400
            },
            "h6": {
                "fontSize": "1.1em",
                "lineHeight": "1em",
                "fontWeight": 600
            },
            "iconSize": "0.8em",
            "sub": {
                "color": "lighten(#666, 20)",
                "fontSize": "luiTheme(reference, sizes, small)",
                "fontWeight": 400
            }
        },
        "links": {
            "color": "#00b2ed",
            "decoration": "none",
            "hover": {
                "color": "lighten(#00b2ed, 10)",
                "decoration": "underline"
            },
            "visited": {
                "color": "darken(#00b2ed, 10)"
            }
        },
        "quote": {
            "fontSize": "1em",
            "fontStyle": "normal",
            "border": {
                "width": "1px",
                "style": "solid",
                "color": "luiTheme(element, divider, color)"
            },
            "margin": {
                "top": "1em",
                "left": 0,
                "right": 0,
                "bottom": "1em"
            },
            "padding": {
                "top": 0,
                "left": "1em",
                "right": 0,
                "bottom": 0
            },
            "cite": {
                "fontSize": "1em",
                "fontStyle": "normal",
                "fontWeight": 400
            }
        },
        "kbd": {
            "fontSize": "0.9em",
            "fontFamily": "monospace",
            "color": "#666",
            "background": "#EFEFEF",
            "borderRadius": "luiTheme(adjective, rounded, radius)",
            "padding": {
                "top": "0.25em",
                "right": "0.5em",
                "bottom": "0.25em",
                "left": "0.5em"
            },
            "border": {
                "width": "1px",
                "style": "solid",
                "color": "darken(#EFEFEF, 10)"
            },
            "extrude": "2px"
        },
        "definitionList": {
            "divider": "':'",
            "gutter": "2em",
            "termWidth": "10em",
            "termWeight": 600,
            "definitionWeight": 400,
            "itemVerticalSpacing": "1em"
        }
    }
}