{
    "theme": {
        "core": {
            "references": {
                "breakpoints": {
                    "mobile": {
                        "break": "320px",
                        "spacing": "1em"
                    },
                    "tablet": {
                        "break": "768px",
                        "spacing": "2em"
                    },
                    "desktop": {
                        "break": "992px",
                        "spacing": "3em"
                    },
                    "large": {
                        "break": "1200px",
                        "spacing": "5em"
                    }
                },
                "palettes": {
                    "colors": {
                        "grey": {
                            "class": "grey",
                            "color": "#666",
                            "text": "#fff"
                        },
                        "light": {
                            "class": "light",
                            "color": "#CCC",
                            "text": "#666"
                        },
                        "primary": {
                            "class": "primary",
                            "color": "#FFC900",
                            "text": "#666"
                        },
                        "secondary": {
                            "class": "secondary",
                            "color": "#F60",
                            "text": "#FFF"
                        },
                        "red": {
                            "class": "red",
                            "color": "#F60",
                            "text": "#FFF"
                        },
                        "orange": {
                            "class": "orange",
                            "color": "#F90",
                            "text": "#FFF"
                        },
                        "green": {
                            "class": "green",
                            "color": "#5cb85c",
                            "text": "#FFF"
                        },
                        "yellow": {
                            "class": "yellow",
                            "color": "#FFC900",
                            "text": "#666"
                        }
                    },
                    "manipulations": {
                        "x-dark": [
                            "darken",
                            25
                        ],
                        "dark": [
                            "darken",
                            15
                        ],
                        "x-light": [
                            "lighten",
                            25
                        ],
                        "light": [
                            "lighten",
                            15
                        ],
                        "accent": [
                            "saturate",
                            "15%"
                        ],
                        "x-accent": [
                            "saturate",
                            "25%"
                        ]
                    }
                },
                "sizes": {
                    "x-small": "0.7em",
                    "small": "0.85em",
                    "regular": "13px",
                    "large": "1.25em",
                    "x-large": "1.5em"
                }
            },
            "adjectives": {
                "animated": {
                    "enabled": true,
                    "transitions": {
                        "animationDuration": "0.25s",
                        "animationEasing": "ease-out",
                        "fadeTranslationDistance": "1em",
                        "scaleAnimationRatio": 25
                    },
                    "static": {
                        "animationDuration": "0.5s",
                        "animationEasing": "ease-out",
                        "bounceDistance": "2em"
                    }
                },
                "pointing": {
                    "enabled": true,
                    "triangleColor": "#CCC",
                    "triangleWidth": "0.5em"
                },
                "raised": {
                    "enabled": true,
                    "default": {
                        "box-shadow": "'0 1px 2px 0 rgba(0, 0, 0, 0.2)'",
                        "filter": "'0 1px 2px rgba(0, 0, 0, 0.2)'"
                    },
                    "high": {
                        "box-shadow": "'0 0 20px 0 rgba(0, 0, 0, 0.2), 0 5px 5px 0 rgba(0, 0, 0, 0.24)'",
                        "filter": "'0 5px 5px rgba(0, 0, 0, 0.24)'"
                    }
                },
                "rounded": {
                    "enabled": true,
                    "radius": "3px"
                },
                "stuck": {
                    "enabled": true,
                    "zIndex": 3
                },
                "unstyled": {
                    "enabled": true
                },
                "visible": {
                    "enabled": true
                }
            },
            "elements": {
                "divider": {
                    "enabled": true,
                    "default": {
                        "width": "1px",
                        "style": "solid",
                        "color": "rgba(50, 50, 50, 0.2)"
                    },
                    "fat": {
                        "width": "3px",
                        "style": "solid",
                        "color": "rgba(50, 50, 50, 0.2)"
                    },
                    "inverted": {
                        "width": "1px",
                        "style": "solid",
                        "color": "rgba(255, 255, 255, 0.2)"
                    }
                },
                "block": {
                    "enabled": true,
                    "verticalPadding": "1em",
                    "horizontalPadding": "1em",
                    "border": {
                        "width": "1px",
                        "style": "solid",
                        "color": "rgba(50, 50, 50, 0.2)"
                    },
                    "invertedBorder": "rgba(255, 255, 255, 0.2)",
                    "borderRadius": "3px"
                },
                "breadcrumbs": {
                    "enabled": true,
                    "itemPadding": "0.5em 0",
                    "dividers": {
                        "fontFamily": "''",
                        "character": "003e",
                        "margin": "0 0.5em",
                        "color": "rgba(50, 50, 50, 0.2)"
                    }
                },
                "typography": {
                    "enabled": true,
                    "body": {
                        "fontFamily": "'Source Sans Pro', 'Tahoma', Verdana, Geneva, Helvetica, Arial, sans-serif",
                        "fontSize": "13px",
                        "color": "#666",
                        "lineHeight": "1.4em",
                        "boldWeight": 600
                    },
                    "smallSize": "0.85em",
                    "headings": {
                        "fontFamily": "'Source Sans Pro', 'Tahoma', Verdana, Geneva, Helvetica, Arial, sans-serif",
                        "lineHeight": "1.25em",
                        "verticalMargin": 0,
                        "fontWeight": "normal",
                        "h1": {
                            "fontSize": "2.75em",
                            "lineHeight": "1.25em",
                            "fontWeight": 300
                        },
                        "h2": {
                            "fontSize": "2.25em",
                            "lineHeight": "1.25em",
                            "fontWeight": 300
                        },
                        "h3": {
                            "fontSize": "1.8em",
                            "lineHeight": "1.3em",
                            "fontWeight": 400
                        },
                        "h4": {
                            "fontSize": "1.5em",
                            "lineHeight": "1.3em",
                            "fontWeight": 400
                        },
                        "h5": {
                            "fontSize": "1.25em",
                            "lineHeight": "1em",
                            "fontWeight": 400
                        },
                        "h6": {
                            "fontSize": "1.1em",
                            "lineHeight": "1em",
                            "fontWeight": 600
                        },
                        "iconSize": "0.8em",
                        "sub": {
                            "color": "lighten(#666, 20)",
                            "fontSize": "0.85em",
                            "fontWeight": 400
                        }
                    },
                    "links": {
                        "color": "#00b2ed",
                        "decoration": "none",
                        "hover": {
                            "color": "lighten(#00b2ed, 10)",
                            "decoration": "underline"
                        },
                        "visited": {
                            "color": "darken(#00b2ed, 10)"
                        }
                    },
                    "quote": {
                        "fontSize": "1em",
                        "fontStyle": "normal",
                        "border": {
                            "width": "1px",
                            "style": "solid",
                            "color": "rgba(50, 50, 50, 0.2)"
                        },
                        "margin": {
                            "top": "1em",
                            "left": 0,
                            "right": 0,
                            "bottom": "1em"
                        },
                        "padding": {
                            "top": 0,
                            "left": "1em",
                            "right": 0,
                            "bottom": 0
                        },
                        "cite": {
                            "fontSize": "1em",
                            "fontStyle": "normal",
                            "fontWeight": 400
                        }
                    },
                    "kbd": {
                        "fontSize": "0.9em",
                        "fontFamily": "monospace",
                        "color": "#666",
                        "background": "#EFEFEF",
                        "borderRadius": "3px",
                        "padding": {
                            "top": "0.25em",
                            "right": "0.5em",
                            "bottom": "0.25em",
                            "left": "0.5em"
                        },
                        "border": {
                            "width": "1px",
                            "style": "solid",
                            "color": "darken(#EFEFEF, 10)"
                        },
                        "extrude": "2px"
                    },
                    "definitionList": {
                        "divider": "':'",
                        "gutter": "2em",
                        "termWidth": "10em",
                        "termWeight": 600,
                        "definitionWeight": 400,
                        "itemVerticalSpacing": "1em"
                    }
                },
                "button": {
                    "enabled": true,
                    "minHeight": "1em",
                    "minWidth": "3em",
                    "verticalPadding": "0.66em",
                    "horizontalPadding": "1em",
                    "verticalMargin": "0.25em",
                    "horizontalMargin": "0.25em",
                    "textTransform": "normal",
                    "textShadow": "none",
                    "borderRadius": "3px",
                    "hoverShadow": "'0 1px 2px 0 rgba(0, 0, 0, 0.2)'",
                    "defaultColoring": {
                        "color": "#FFFFFF",
                        "text": "#666"
                    },
                    "defaultBorderWidth": "1px",
                    "defaultBorderColor": "rgba(50, 50, 50, 0.2)",
                    "inverted": {
                        "color": "rgba(200, 200, 200, 0.75)"
                    },
                    "flat": {
                        "background": "transparent",
                        "textDecoration": "underline",
                        "hoverBackground": "rgba(0, 0, 0, 0.05)",
                        "activeBackground": "rgba(0, 0, 0, 0.1)",
                        "activeShadow": "0 0 0 1px rgba(50, 50, 50, .1)",
                        "invertedHoverBackground": "rgba(200, 200, 200, 0.1)",
                        "invertedActiveBackground": "rgba(255, 255, 255, .25)"
                    },
                    "wired": {
                        "borderWidth": "2px"
                    },
                    "disabledOpacity": 0.5,
                    "disabledGrayscale": "50%",
                    "addon": {
                        "width": "2.5em",
                        "background": "rgba(0, 0, 0, 0.1)"
                    }
                },
                "callout": {
                    "enabled": true,
                    "background": "#FFF",
                    "borderColor": "#CCC",
                    "borderWidth": "5px",
                    "verticalPadding": "1em",
                    "horizontalPadding": "2em",
                    "verticalMargin": "1em",
                    "horizontalMargin": 0,
                    "defaultColoring": {
                        "color": "#CCC",
                        "text": "#222"
                    }
                },
                "dayblock": {
                    "enabled": true,
                    "width": "5em",
                    "verticalMargin": "1em",
                    "horizontalMargin": 0,
                    "textTransform": "uppercase",
                    "textAlign": "center",
                    "textColor": "#666",
                    "borderRadius": "3px",
                    "borderWidth": "1px",
                    "borderColor": "rgba(50, 50, 50, 0.2)",
                    "day": {
                        "verticalPadding": "0.25em",
                        "fontSize": "3em",
                        "fontWeight": 600
                    },
                    "month": {
                        "verticalPadding": 0,
                        "fontSize": "1.5em",
                        "fontWeight": 600
                    },
                    "year": {
                        "verticalPadding": "0.1em",
                        "fontSize": "1em",
                        "fontWeight": 400
                    }
                },
                "grid": {
                    "enabled": true,
                    "gutter": "1em",
                    "columns": 12,
                    "containerWidth": "auto"
                },
                "icon": {
                    "enabled": true,
                    "createClasses": true,
                    "iconBaseSelector": "'#{$prefix}.icon'"
                },
                "input": {
                    "enabled": true,
                    "background": "#FFF",
                    "borderRadius": "3px",
                    "color": "#666",
                    "padding": {
                        "top": "0.66em",
                        "right": "1em",
                        "bottom": "0.66em",
                        "left": "1em"
                    },
                    "borderWidth": "1px",
                    "borderColor": "#CCC",
                    "focusBorderColor": "darken(#CCC, 15)",
                    "invalidColor": "#F60",
                    "shadow": "'0 1px 2px 0 rgba(0, 0, 0, 0.2)'",
                    "placeholder": {
                        "color": "rgba(0, 0, 0, 0.5)",
                        "style": "italic"
                    },
                    "field": {
                        "verticalMargin": "0.5em",
                        "labelFontWeight": 600
                    },
                    "lengths": {
                        "x-short": "5em",
                        "short": "8em",
                        "medium": "15em",
                        "long": "20em",
                        "x-long": "25em"
                    },
                    "checkbox": {
                        "enabled": true,
                        "color": "#FFC900",
                        "size": "1.5em"
                    },
                    "radio": {
                        "enabled": true,
                        "color": "#FFC900",
                        "size": "1.5em",
                        "lineHeight": 1.5
                    },
                    "switch": {
                        "enabled": true,
                        "width": "4.5em",
                        "height": "2.25em",
                        "background": "#666",
                        "color": "darken(#666, 15)",
                        "activeBackground": "#FFC900",
                        "activeColor": "darken(#FFC900, 15)",
                        "grabLinesColor": "#CCC"
                    },
                    "search": {
                        "enabled": true,
                        "paddingLeft": "2.5em",
                        "tag": {
                            "padding": "0.25em 0.5em",
                            "margin": "0.1em",
                            "background": "lighten(#CCC, 15)",
                            "borderColor": "#CCC",
                            "borderRadius": "3px",
                            "selectedColor": "#FFC900"
                        }
                    },
                    "timespanpicker": {
                        "enabled": true,
                        "width": "5em"
                    },
                    "momentpicker": {
                        "enabled": true,
                        "width": "2.2em",
                        "maxWidth": "5.25em"
                    },
                    "translations": {
                        "background": "#FFF",
                        "dropdownPadding": "0.5em",
                        "zIndex": 3
                    }
                },
                "label": {
                    "enabled": true,
                    "fontSize": "0.85em",
                    "lineHeight": 1,
                    "textAlign": "center",
                    "verticalPadding": "0.25em",
                    "horizontalPadding": "0.5em",
                    "verticalMargin": 0,
                    "horizontalMargin": "0.5em",
                    "borderRadius": "3px",
                    "defaultColoring": {
                        "color": "#CCC",
                        "text": "#666"
                    },
                    "addonBackground": "rgba(0, 0, 0, 0.2)",
                    "iconMargin": "0.25em"
                },
                "menu": {
                    "enabled": true,
                    "defaultColoring": {
                        "color": "#666",
                        "text": "#FFF"
                    },
                    "verticalMargin": 0,
                    "horizontalMargin": 0,
                    "verticalPadding": 0,
                    "horizontalPadding": 0,
                    "borderColor": "rgba(50, 50, 50, 0.2)",
                    "invertedBorderColor": "rgba(255, 255, 255, 0.2)",
                    "borderStyle": "solid",
                    "borderWidth": "1px",
                    "item": {
                        "vertical": {
                            "verticalPadding": "13px",
                            "horizontalPadding": "19.5px",
                            "verticalMargin": 0,
                            "horizontalMargin": 0
                        },
                        "horizontal": {
                            "verticalPadding": "13px",
                            "horizontalPadding": "19.5px",
                            "verticalMargin": 0,
                            "horizontalMargin": 0
                        },
                        "header": {
                            "fontSize": "''"
                        }
                    },
                    "dividing": {
                        "itemColor": "#666",
                        "dividerColor": "rgba(50, 50, 50, 0.2)",
                        "dividerWidth": "1px",
                        "activeDividerWidth": "3px",
                        "activeDividerColor": "#FFC900"
                    },
                    "tabbed": {
                        "verticalPadding": 0,
                        "horizontalPadding": "13px",
                        "background": "#FAFAFA",
                        "activeBackground": "#FFF",
                        "borderColor": "rgba(50, 50, 50, 0.2)",
                        "activeBorderWidth": "3px",
                        "bottomSpacing": "5px"
                    },
                    "pills": {
                        "borderRadius": "3px"
                    }
                },
                "notify": {
                    "enabled": true,
                    "max-width": "90%",
                    "z-index": 9999
                },
                "overlay": {
                    "enabled": true,
                    "z-index": 100,
                    "background": "rgba(0, 0, 0, 0.5)"
                },
                "progress": {
                    "enabled": true,
                    "height": "1.5em",
                    "slimHeight": "5px",
                    "verticalMargin": "1em",
                    "borderRadius": "3px",
                    "background": "#EFEFEF",
                    "defaultColoring": "light",
                    "status": {
                        "fontWeight": 600
                    },
                    "progressBar": {
                        "zIndex": 1000,
                        "height": "2px",
                        "fadeInDuration": "0.25s"
                    }
                },
                "table": {
                    "enabled": true,
                    "background": "#fff",
                    "border": "1px solid '#d6d6d6'",
                    "padding": "0.5em 1em",
                    "zebraBackground": "rgba(200, 200, 200,0.25)",
                    "verticalMargin": "1em",
                    "horizontalMargin": 0,
                    "textAlignment": "left",
                    "coloredBorderWidth": "5px"
                },
                "tablegrid": {
                    "background": "#FFF",
                    "textAlignment": "left",
                    "toolbar": {
                        "background": "#666",
                        "text": "#fff"
                    },
                    "header": {
                        "background": "lighten(#CCC, 15)",
                        "borderColor": "#CCC",
                        "text": "lighten(#666, 15)",
                        "cellPaddingTop": "2em",
                        "fontWeight": "normal",
                        "textAlignment": "left"
                    },
                    "rows": {
                        "zebraColor": "lighten(#CCC, 15)",
                        "hoverColor": "lighten(#FFC900, 25)"
                    },
                    "cell": {
                        "minWidth": "5em",
                        "padding": "0.5em 1em"
                    }
                }
            }
        }
    }
}