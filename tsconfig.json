	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"removeComments": true,
		"sourceMap": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		"bower_components",
		"dist"
	]
}